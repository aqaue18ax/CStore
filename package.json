{
  "name": "cstore",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "babel-plugin-import": "^1.12.0",
    "babel-polyfill": "^6.26.0",
    "core-js": "^2.6.5",
    "echarts": "^4.8.0",
    "echarts-gl": "^1.1.1",
    "lib-flexible": "^0.3.2",
    "postcss-pxtorem": "^4.0.1",
    "v-charts": "^1.19.0",
    "vant": "2.5.5",
    "vue": "^2.6.10",
    "vue-amap": "^0.5.9",
    "vue-awesome-swiper": "^3.1.3",
    "vue-router": "^3.0.3",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
          "Android >= 4.0",
          "iOS >= 7"
        ]
      },
      "postcss-pxtorem": {
        "rootValue": 30,
        "propList": [
          "*",
          "!border",
          "!border-radius",
          "!box-shadow",
          "!--shadowsize",
          "!border-top-left-radius",
          "!border-top-right-radius",
          "!border-bottom-left-radius",
          "!border-bottom-right-radius"
        ],
        "selectorBlackList": [
          "van-",
          "solid"
        ]
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
